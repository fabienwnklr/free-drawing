{
  "name": "@fabwcie/free-drawing",
  "author": "Winkler Fabien <fabienwinkler@outlook.fr>",
  "description": "Infinite canvas",
  "contributors": [],
  "private": false,
  "version": "1.0.1-alpha.1",
  "type": "module",
  "main": "src/Drawer.ts",
  "types": "./dist/index.d.ts",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=5.7.1"
  },
  "exports": {
    ".": {
      "import": "./dist/drawer.js",
      "require": "./dist/drawer.umd.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/fabienwnklr/free-drawing.git"
  },
  "keywords": [
    "typescript"
  ],
  "files": [
    "dist/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "build:watch": "tsc && vite build --watch",
    "build:static": "tsc && vite build --mode static",
    "doc:typedoc": "npx typedoc --options typedoc.json src/**/*.ts",
    "doc:changelog": "cp CHANGELOG.md docs/changelog.md",
    "doc:get-started": "cp README.md docs/get-started/index.md",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "lint": "npx eslint src/**",
    "format": "prettier src/**/*.ts --write",
    "preview": "vite preview",
    "release": "yarn build && commit-and-tag-version && git push --follow-tags origin master && yarn publish",
    "release:patch": "yarn build && commit-and-tag-version --release-as patch && yarn publish",
    "release:minor": "yarn build && commit-and-tag-version --release-as minor && yarn publish",
    "release:major": "yarn build && commit-and-tag-version --release-as major && yarn publish",
    "publish": "git push --follow-tags origin master && npm publish --access public",
    "docs": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.2",
    "@commitlint/config-conventional": "^17.7.0",
    "@playwright/test": "^1.39.0",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "commit-and-tag-version": "^11.3.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "jest-canvas-mock": "^2.5.2",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.3",
    "sass": "^1.69.1",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^4.0.0-next.55",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-dts": "^3.6.0",
    "vite-svg-loader": "^5.1.0",
    "vitepress": "^1.0.0-rc.42",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "konva": "^9.3.1",
    "perfect-freehand": "^1.2.0"
  }
}
